<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<ExtensionManifest xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" ExtensionBundleId="com.julian.kiproSequences" ExtensionBundleVersion="1.0" Version="12.0">
	<ExtensionList>
		<Extension Id="com.julian.kiproSequences" Version="1.0.0" />
	</ExtensionList>

	<ExecutionEnvironment>
		<HostList>
			<Host Name="PPRO" Version="9.0" />
		</HostList>
		<LocaleList>
			<Locale Code="All" />
		</LocaleList>
		<RequiredRuntimeList>
			<RequiredRuntime Name="CSXS" Version="12.0" />
		</RequiredRuntimeList>
	</ExecutionEnvironment>

	<DispatchInfoList>
		<Extension Id="com.julian.kiproSequences">
			<DispatchInfo >
				<Resources>
					<MainPath>./index.html</MainPath>
					<ScriptPath>./KiProSequences.jsx</ScriptPath>
					<CEFCommandLine>
						<Parameter>--allow-file-access</Parameter>
						<Parameter>--allow-file-access-from-files</Parameter>
						<Parameter>--allow-running-insecure-content</Parameter>
						<Parameter>--enable-media-stream</Parameter>
						<Parameter>--enable-nodejs</Parameter>
					</CEFCommandLine>
				</Resources>
				<Lifecycle>
					<AutoVisible>true</AutoVisible>
				</Lifecycle>
				<UI>
					<Type>Panel</Type>
					<Menu>KiPro Sequences</Menu>
					<Geometry>
						<Size>
							<Height>300</Height>
							<Width>500</Width>
						</Size>
                        <MaxSize>
                            <Height>800</Height>
                            <Width>1200</Width>
                        </MaxSize>						
						<MinSize>
                            <Height>200</Height>
                            <Width>200</Width>
                        </MinSize>
					</Geometry>
					<Icons>
                        <Icon Type="Normal">./lib/icons/TeEx_HTML_TEST_23x23_N.png</Icon>
                        <Icon Type="RollOver">./lib/icons/TeEx_HTML_TEST_23x23_R.png</Icon>
                        <Icon Type="DarkNormal">./lib/icons/TeEx_HTML_TEST_23x23_DN.png</Icon>
                        <Icon Type="DarkRollOver">./lib/icons/TeEx_HTML_TEST_23x23_DR.png</Icon>
                    </Icons>
				</UI>
			</DispatchInfo>
		</Extension>
	</DispatchInfoList>

</ExtensionManifest>
